<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.remake">
<!--    인터넷 사용 권한 부여-->
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/icon_application"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_application_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Remake">
        <activity
            android:name=".activitys.SplashActivity"
            android:theme="@style/SplashTheme"
            android:label="Zer0"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.MainActivity"
            android:exported="true">
        </activity>

        <activity android:name=".activitys.SubActivity"/>
        <activity android:name=".activitys.DrinkActivity"/>
        <activity android:name=".activitys.WebActivity"/>
    </application>
</manifest>